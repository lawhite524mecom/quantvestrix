{
  "name": "quantvestrix",
  "version": "2025.8.15.91819",
  "description": "QUANTVESTRIX DNA Blockchain - Revolutionary blockchain with biological DNA storage and quantum processing",
  "main": "index.js",
  "scripts": {
    "start": "node index.js",
    "dev": "nodemon index.js",
    "test": "jest",
    "build": "webpack --mode production",
    "deploy": "npm run build && pm2 start ecosystem.config.js",
    "install:all": "npm install && cd quantvestrix-core && npm install && cd ../quantvestrix-wallet && npm install",
    "start:all": "concurrently \"npm run start:core\" \"npm run start:wallet\" \"npm run start:network\"",
    "start:core": "cd quantvestrix-core && node src/core-node/quantvestrix_core_node.js",
    "start:wallet": "cd quantvestrix-wallet && python3 -m http.server 8080",
    "start:network": "cd quantvestrix-core && node src/network-node/quantvestrix_network_node.js"
  },
  "keywords": [
    "blockchain",
    "dna",
    "quantum",
    "cryptocurrency",
    "decentralized",
    "full-burst",
    "multi-chain",
    "universal-connector",
    "gps-tracking",
    "component-injection"
  ],
  "author": "QUANTVESTRIX Development Team",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "ws": "^8.14.2",
    "crypto": "^1.0.1",
    "axios": "^1.6.0",
    "alchemy-sdk": "^3.0.0",
    "@solana/web3.js": "^1.87.6",
    "ethers": "^6.8.1",
    "bitcoinjs-lib": "^6.1.5",
    "geolib": "^3.3.4",
    "concurrently": "^8.2.2",
    "nodemon": "^3.0.1",
    "jest": "^29.7.0",
    "webpack": "^5.89.0",
    "pm2": "^5.3.0"
  },
  "devDependencies": {
    "webpack-cli": "^5.1.4"
  },
